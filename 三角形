#include<stdio.h>
#include<math.h>
int main(){
	int a, b, c;
	scanf("%d%d%d", &a, &b, &c);
	if (a+b>c && a+c>b && b+c>a && abs(b-a)<c && abs(b-c)<a && abs(a-c)<b){
		if (a*a+b*b==c*c || c*c+b*b==a*a || a*a+c*c==b*b) printf("yes");
		else printf("no");
	}
	else printf("not a triangle");
	return 0;
}
